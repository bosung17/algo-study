package dwarf;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		
		int[] height = new int[9];
		int sum = 0;
		
		Scanner sc = new Scanner(System.in);
		
		for (int i=0; i<9; i++) {
			height[i] = sc.nextInt();
			sum += height[i];
		}
		System.out.println(sum);
	}

}
